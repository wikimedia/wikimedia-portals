<div class="central-textlogo wikipedia25-videologo">
    <video id="wikipedia25-video" class="wikipedia25-video"
        data-sneakpeek-light-200="{{l10n.assets.videoLogo.sneakpeek.srcLight200}}"
        data-sneakpeek-light-300="{{l10n.assets.videoLogo.sneakpeek.srcLight300}}"
        data-sneakpeek-dark-200="{{l10n.assets.videoLogo.sneakpeek.srcDark200}}"
        data-sneakpeek-dark-300="{{l10n.assets.videoLogo.sneakpeek.srcDark300}}"
        data-balloons-light-200="{{l10n.assets.videoLogo.balloons.srcLight200}}"
        data-balloons-light-300="{{l10n.assets.videoLogo.balloons.srcLight300}}"
        data-balloons-dark-200="{{l10n.assets.videoLogo.balloons.srcDark200}}"
        data-balloons-dark-300="{{l10n.assets.videoLogo.balloons.srcDark300}}" data-width="200" data-height="200"
        width="200" height="200" autoplay loop muted playsinline
        poster="{{l10n.assets.logo.src}}">
    </video>
    <h1 class="central-textlogo-wrapper">
        <span class="central-textlogo__image sprite svg-{{l10n.portal.name}}_wordmark">
            {{l10n.portal.name}}
        </span>
        <strong class="jsl10n wikipedia25-slogan"
            data-jsl10n="portal.wikipedia25.slogan">{{l10n.portal.wikipedia25.slogan}}</strong>
    </h1>
</div>
